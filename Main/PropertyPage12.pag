Private Sub Command2_Click()
Dim iXY As Long, iY As Long, TmpCount As Long

PopPg.FixedColsText(Text1(0).Text) = Text2(0).Text
PopPg.FixedRowsText(Text1(1).Text) = Text2(1).Text

PopPg.SellWidth(Text1(0).Text, True) = Text3(0).Text
PopPg.SellHeight(Text1(1).Text, True) = Text3(1).Text

PopPg.SellText(Text1(0).Text, Text1(1).Text) = Text3(2).Text
PopPg.SellSubText(Text1(0).Text, Text1(1).Text) = Text3(3).Text

'PopPg.FixdColnDefColStyl = Combo2(0).ListIndex
PopPg.FixdColnColStyl(Text1(0).Text) = Combo2(0).ListIndex
PopPg.FixdColnColGradn Text1(0).Text, ColX(0), ColX(1)

PopPg.FixdRowsColStyl(Text1(1).Text) = Combo2(1).ListIndex
PopPg.FixdRowsColGradn Text1(1).Text, ColY(0), ColY(1)

'PopPg.FixdColnColBck0(Text1(0).Text) = ColX(0) 'err perbaikan untuk penempatan gradient
'PopPg.FixdColnColBck1(Text1(0).Text) = ColX(1)
'poppg.FixdColnDefColGradn =

