Private Sub UpDown1_Change(Index As Integer)
If Index = 0 Then
    If UpDown1(0).Value > PopPg.SellCountColumn - 1 Then UpDown1(0).Value = PopPg.SellCountColumn - 1: Exit Sub
    PopPg.GridLeft = UpDown1(0).Value
End If
If Index = 1 Then
    If UpDown1(1).Value > PopPg.SellCountRow - 1 Then UpDown1(1).Value = PopPg.SellCountRow - 1: Exit Sub
    PopPg.GridUp = UpDown1(1).Value
    
    Text4.Text = (PopPg.SellCountRow - 1) - UpDown1(1).Value
End If
OutText

