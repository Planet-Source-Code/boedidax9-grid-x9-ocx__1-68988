Private Sub Text1_Change(Index As Integer)
If Text1(Index) = "" Then Exit Sub
If Index = 0 Then
On Error GoTo xErr
        UpDown1(0).Value = Text1(0).Text
    Exit Sub
xErr:
UpDown1(0).Value = PopPg.SellCountColumn - 1
ElseIf Index = 1 Then
On Error GoTo yErr
        UpDown1(1).Value = Text1(1).Text
    Exit Sub
yErr:
UpDown1(1).Value = PopPg.SellCountRow - 1
End If

End Sub

