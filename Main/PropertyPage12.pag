'PopPg.FixdColnColBck1(Text2(0).Text) = ColY(1)

'PopPg.FixdColnForeColor = Picture1(1).BackColor
'PopPg.FixdRowsDefColGradn = Picture1(2).BackColor
'PopPg.FixdRowsForeColor = Picture1(3).BackColor

If Combo1(0).ListIndex = 0 Or Combo1(1).ListIndex = 0 Then
    If Combo1(0).ListIndex = 0 Then SellText Text1(0).Text, Text1(1).Text
    If Combo1(1).ListIndex = 0 Then SellTextSub Text1(0).Text, Text1(1).Text
End If
If Combo1(0).ListIndex = 1 Or Combo1(1).ListIndex = 1 Then
    For iXY = 0 To PopPg.SellCountColumn - 1
        If Combo1(0).ListIndex = 1 Then SellText iXY, Text1(1).Text
        If Combo1(1).ListIndex = 1 Then SellTextSub iXY, Text1(1).Text
    Next iXY
End If
If Combo1(0).ListIndex = 2 Or Combo1(1).ListIndex = 2 Then
    For iXY = 0 To PopPg.SellCountRow - 1
        If Combo1(0).ListIndex = 2 Then SellText Text1(0).Text, iXY
        If Combo1(1).ListIndex = 2 Then SellTextSub Text1(0).Text, iXY
    Next iXY
End If

PopPg.GoDrawing

OutText
End Sub

Sub SellText(NewX As Long, NewY As Long)
    If Shape2(4).Visible = False Then _
        PopPg.SellBackColor(NewX, NewY) = Picture1(4).BackColor
        PopPg.SellNoFill(NewX, NewY, 0) = ConfertBackBoolean(Shape2(4).Visible)
    If Shape2(5).Visible = False Then _
        PopPg.SellForeColor(NewX, NewY) = Picture1(5).BackColor
        PopPg.SellNoFill(NewX, NewY, 1) = ConfertBackBoolean(Shape2(5).Visible)
    
    PopPg.SellBold(NewX, NewY) = Check2(0).Value
    PopPg.SellItalic(NewX, NewY) = Check2(1).Value
    PopPg.SellUnderline(NewX, NewY) = Check2(2).Value
End Sub
Sub SellTextSub(NewX As Long, NewY As Long)
    If Shape2(6).Visible = False Then _
        PopPg.SellSubBackColor(NewX, NewY) = Picture1(6).BackColor
        PopPg.SellNoFillSub(NewX, NewY, 0) = ConfertBackBoolean(Shape2(6).Visible)
    
    If Shape2(7).Visible = False Then _
        PopPg.SellSubForeColor(NewX, NewY) = Picture1(7).BackColor
        PopPg.SellNoFillSub(NewX, NewY, 1) = ConfertBackBoolean(Shape2(7).Visible)
    
    PopPg.SellSubBold(NewX, NewY) = Check3(0).Value
    PopPg.SellSubItalic(NewX, NewY) = Check3(1).Value
    PopPg.SellSubUnderline(NewX, NewY) = Check3(2).Value
End Sub


