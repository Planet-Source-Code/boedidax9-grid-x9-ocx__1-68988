Dim PopPg As DBGrid
Dim ColX() As OLE_COLOR, ColY() As OLE_COLOR

Private Sub Combo2_Click(Index As Integer)
If Index = 0 Then GHJ Index, 0, ColX(), PopPg.FixdColnDefColStyl, PopPg.FixdColnDefColBck0, PopPg.FixdColnDefColBck1
If Index = 1 Then GHJ Index, 2, ColY(), PopPg.FixdRowsDefColStyl, PopPg.FixdRowsDefColBck0, PopPg.FixdRowsDefColBck1
Exit Sub
If Index = 0 Then
    Select Case Combo2(Index).ListIndex
    Case 0
        If PopPg.FixdColnDefColStyl = 0 Then
            Picture1(0).BackColor = PopPg.FixdColnDefColBck0
        ElseIf PopPg.FixdColnDefColStyl = 1 Or PopPg.FixdColnDefColStyl = 2 Then
            Picture1(0).Cls
            PopPg.pvDrawGrid Picture1(0), 0, 0, Picture1(0).ScaleWidth, Picture1(0).ScaleHeight, PopPg.FixdColnDefColBck0, PopPg.FixdColnDefColBck1, PopPg.FixdColnDefColStyl - 1
        Else
            'picture
        End If
    Case 1
        Picture1(0).BackColor = ColX(0)
    Case 2, 3
        Picture1(0).Cls
        PopPg.pvDrawGrid Picture1(0), 0, 0, Picture1(0).ScaleWidth, Picture1(0).ScaleHeight, ColX(0), ColX(1), Combo2(Index).ListIndex - 2
    Case 4
    
    End Select
End If
End Sub

Sub GHJ(IndexCombo As Integer, PicIndex As Integer, Col() As OLE_COLOR, DefSty As Integer, DefCol0 As Long, DefCol1 As Long)
Select Case Combo2(IndexCombo).ListIndex
Case 0
    If DefSty = 0 Then
        Picture1(PicIndex).BackColor = DefCol0
    ElseIf DefSty = 1 Or DefSty = 2 Then
        Picture1(PicIndex).Cls
        PopPg.pvDrawGrid Picture1(PicIndex), 0, 0, Picture1(PicIndex).ScaleWidth, Picture1(PicIndex).ScaleHeight, DefCol0, DefCol1, DefSty - 1
    Else
        'picture
    End If
Case 1
    Picture1(PicIndex).BackColor = Col(0)
Case 2, 3
    Picture1(PicIndex).Cls
    PopPg.pvDrawGrid Picture1(PicIndex), 0, 0, Picture1(PicIndex).ScaleWidth, Picture1(PicIndex).ScaleHeight, Col(0), Col(1), Combo2(IndexCombo).ListIndex - 2
Case 4
    'picture
End Select
End Sub

Private Sub Command1_Click()
If Command1.Caption = "Open" Then
    Command1.Caption = "Close"
    PopPg.SizeUpDown Text1(1).Text, True
Else
