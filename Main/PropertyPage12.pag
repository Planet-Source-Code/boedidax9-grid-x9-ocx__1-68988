Private Sub Picture1_MouseDown(Index As Integer, Button As Integer, Shift As Integer, X As Single, Y As Single)
Dim Col() As OLE_COLOR

ReDim Col(1)

If Button = 1 Then
If Combo2(0).ListIndex = 0 And Picture1(Index).Tag <> "" Then Exit Sub

CommonDialog1.CancelError = True
On Error GoTo err
'up date this combo col...
    CommonDialog1.ShowColor
    Col(0) = CommonDialog1.Color
        Picture1(Index).BackColor = CommonDialog1.Color
        Shape2(Index).Visible = False
        
        If (Combo2(Val(Picture1(Index).Tag)).ListIndex = 2 Or Combo2(Val(Picture1(Index).Tag)).ListIndex = 3) _
           And Picture1(Index).Tag <> "" Then
            CommonDialog1.ShowColor
            Col(1) = CommonDialog1.Color
            
            Picture1(Index).Cls
            PopPg.pvDrawGrid Picture1(Index), 0, 0, Picture1(Index).ScaleWidth, Picture1(Index).ScaleHeight, Col(0), Col(1), Combo2(Val(Picture1(Index).Tag)).ListIndex - 2
            Shape2(Index).Visible = False
        End If

        If Index = 0 Then
            If Combo2(Val(Picture1(Index).Tag)).ListIndex = 2 Or Combo2(Val(Picture1(Index).Tag)).ListIndex = 3 Then ColX() = Col() Else ColX(0) = Col(0) 'update this ok
        ElseIf Index = 2 Then
            If Combo2(Val(Picture1(Index).Tag)).ListIndex = 2 Or Combo2(Val(Picture1(Index).Tag)).ListIndex = 3 Then ColY() = Col() Else ColY(0) = Col(0)
        End If
err:
Exit Sub

ElseIf Button = 2 Then
    If Index >= 4 Then
    '>>>>>>>>> 'ghh ghfg hggh gggh hg hgg h hghgh hh hhhhhhhhhhhhhhhhhhhhhhhhh.
        Select Case Index
        Case 4
            Picture1(4).BackColor = PopPg.SellBackColor_Def
            Shape2(Index).Visible = True
        Case 5
            Picture1(5).BackColor = PopPg.SellForeColor_Def
            Shape2(Index).Visible = True
        Case 6
            Picture1(6).BackColor = PopPg.SellSubBackColor_Def
            Shape2(Index).Visible = True
        Case 7
            Picture1(7).BackColor = PopPg.SellSubForeColor_Def
            Shape2(Index).Visible = True
        End Select
    End If
End If
End Sub

