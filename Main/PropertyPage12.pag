Else
    Picture1(7).BackColor = PopPg.SellSubForeColor(Text1(0).Text, Text1(1).Text)
    Shape2(7).Visible = False
End If

Label2(0).Alignment = PopPg.SellAlignment(Text1(0).Text, Text1(1).Text)
Label2(1).Alignment = PopPg.SellSubAlignment(Text1(0).Text, Text1(1).Text)

Check2(0).Value = ConfertBoolean(PopPg.SellBold(Text1(0).Text, Text1(1).Text))
Check2(1).Value = ConfertBoolean(PopPg.SellItalic(Text1(0).Text, Text1(1).Text))
Check2(2).Value = ConfertBoolean(PopPg.SellUnderline(Text1(0).Text, Text1(1).Text))

Check3(0).Value = ConfertBoolean(PopPg.SellSubBold(Text1(0).Text, Text1(1).Text))
Check3(1).Value = ConfertBoolean(PopPg.SellSubItalic(Text1(0).Text, Text1(1).Text))
Check3(2).Value = ConfertBoolean(PopPg.SellSubUnderline(Text1(0).Text, Text1(1).Text))


If PopPg.SellOpenHeight(Text1(1).Text) = False Then Command1.Caption = "Open" Else Command1.Caption = "Close"
End Sub

Function ConfertBoolean(NewValue As Boolean) As Integer
    If NewValue = False Then ConfertBoolean = 0 Else ConfertBoolean = 1
End Function

Function ConfertBackBoolean(NewValue As Boolean) As Boolean
    If NewValue = False Then ConfertBackBoolean = True Else ConfertBackBoolean = False
End Function

Sub GHT(PicIndex As Integer, Col() As OLE_COLOR, DefSty As Integer, DefCol0 As Long, DefCol1 As Long, Sty As Integer, Col0 As Long, Col1 As Long)
    If Sty = 0 Then
        If DefSty = 0 Then Picture1(PicIndex).BackColor = DefCol0
        If DefSty = 1 Or DefSty = 2 Then
            Picture1(PicIndex).Cls
            PopPg.pvDrawGrid Picture1(PicIndex), 0, 0, Picture1(PicIndex).ScaleWidth, Picture1(PicIndex).ScaleHeight, DefCol0, DefCol1, DefSty - 1
        End If
    ElseIf Sty = 1 Then
        Picture1(PicIndex).BackColor = Col0
    ElseIf Sty = 2 Or Sty = 3 Then
        Picture1(PicIndex).Cls
        PopPg.pvDrawGrid Picture1(PicIndex), 0, 0, Picture1(PicIndex).ScaleWidth, Picture1(PicIndex).ScaleHeight, Col0, Col1, Sty - 2
        Col(0) = Col0
        Col(1) = Col1
    ElseIf Sty = 4 Then
        MsgBox "picture"
    End If

Combo2(Val(Picture1(PicIndex).Tag)).ListIndex = Sty
End Sub

